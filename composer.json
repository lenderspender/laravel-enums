{
    "name": "lenderspender/laravel-enums",
    "description": "Add enums to Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LenderSpender\\LaravelEnums\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\LaravelEnums\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "barryvdh/reflection-docblock": "^2.0.6",
        "composer/composer": "^2.0.13",
        "illuminate/support": "^7.0 | ^8.0",
        "illuminate/translation": "^7.0 | ^8.0",
        "illuminate/validation": "^7.0 | ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.0 | ^6.0",
        "lenderspender/php-cs-fixer-rules": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LenderSpender\\LaravelEnums\\LaravelEnumsServiceProvider"
            ]
        }
    }
}
