{
    "name": "lenderspender/laravel-enums",
    "description": "Add enums to Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LenderSpender\\LaravelEnums\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\LaravelEnums\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "barryvdh/reflection-docblock": "^v2.0.6",
        "composer/composer": "^2",
        "illuminate/support": "^v10.0",
        "illuminate/translation": "^v10.0",
        "illuminate/validation": "^v10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^8.0",
        "mheap/phpunit-github-actions-printer": "^1.2",
        "lenderspender/php-cs-fixer-rules": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LenderSpender\\LaravelEnums\\LaravelEnumsServiceProvider"
            ]
        }
    }
}
